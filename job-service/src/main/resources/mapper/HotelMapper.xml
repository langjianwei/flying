<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ljw.crawler.mapper.JobMapper">


    <select id="getSkus" resultType="com.ljw.crawler.entity.Skus">
        SELECT
        *
        FROM
        skus
        WHERE
        fetched = TRUE limit 10 offset 0
    </select>

</mapper>